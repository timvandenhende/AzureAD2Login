{
  "title": "Login",
  "version": "1.00",
  "name": "Login",
  "description": "Plugin that alows a user to sign in trough Azure AD 2.0 and Google.",
  "author": "Tim Vanden Hende",
  "options": [
    {
      "label": "client_id",
      "name": "client_id",
      "type": "Text",
      "default": ""
    },
    {
      "label": "scope",
      "name": "scope",
      "type": "Text",
      "default": "offline_access%20https://graph.microsoft.com/user.read"
    },
    {
      "label": "client_secret",
      "name": "client_secret",
      "type": "Text",
      "default": ""
    },
    {
      "label": "GoogleLoginScope",
      "name": "GoogleLoginScope",
      "type": "Text",
      "default": "https%3A%2F%2Fwww.googleapis.com%2Fauth%2Fdrive.metadata.readonly"
    },
    {
      "label": "Googleclient_id",
      "name": "Googleclient_id",
      "type": "Text",
      "default": ""
    },
    {
      "label": "Googleclient_secret",
      "name": "Googleclient_secret",
      "type": "Text",
      "default": ""
    }
  ]
}